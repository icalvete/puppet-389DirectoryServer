[General]
FullMachineName= <%= fqdn %>
SuiteSpotUserID= <%= scope.lookupvar('ds389::params::ds_user') %>
SuiteSpotGroup= <%= scope.lookupvar('ds389::params::ds_group') %>
AdminDomain= <%= ldap_domain %>
ConfigDirectoryAdminID= admin
ConfigDirectoryAdminPwd= <%= ldap_admin_password %>
ConfigDirectoryLdapURL= ldap://<%= fqdn -%>:389/o=NetscapeRoot

[slapd]
SlapdConfigForMC= Yes
UseExistingMC= 0
ServerPort= 389
ServerIdentifier= dir
Suffix= <%= ldap_suffix %>
RootDN= <%= ldap_admin_user %>
RootDNPwd= <%= ldap_admin_password %>
AddSampleEntries= No

